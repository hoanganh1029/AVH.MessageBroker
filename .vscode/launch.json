{
   "version": "0.2.0",
   "configurations": [
      {
         "name": ".NET Core Launch (console)",
         "type": "coreclr",
         "request": "launch",
         "preLaunchTask": "build",
         "program": "${workspaceFolder}/AVH.MessageBroker.AzureMessaging/bin/Debug/net6.0/AVH.MessageBroker.AzureMessaging.dll",
         "args": [],
         "cwd": "${workspaceFolder}/AVH.MessageBroker.AzureMessaging",
         "console": "internalConsole",
         "stopAtEntry": false
      },
      {
         "name": ".NET Core Attach",
         "type": "coreclr",
         "request": "attach"
      },
      {
         "name": "Docker .NET Launch",
         "type": "docker",
         "request": "launch",
         "preLaunchTask": "docker-run: debug",
         "netCore": {
            "appProject": "${workspaceFolder}/AVH.MessageBroker.Worker/AVH.MessageBroker.Worker.csproj"
         }
      }
   ]
}